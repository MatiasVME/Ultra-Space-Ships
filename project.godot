; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Ultra Space Ships"
run/main_scene="res://Game/MainScreens/Splash.tscn"
config/icon="res://icon.png"

[autoload]

Main="*res://Game/Autoload/Main.gd"
MusicManager="*res://Game/Autoload/MusicManager.tscn"
SoundManager="*res://Game/Autoload/SoundManager.tscn"

[display]

window/size/width=800
window/size/height=480
mouse_cursor/custom_image="res://Game/Cursor/cursor.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

2d/default_gravity=0
2d/default_gravity_vector=Vector2( 0, 0 )

[rendering]

environment/default_environment="res://default_env.tres"
